{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "vsdocsdocs/**",
          "**/includes/**"
        ],
        "src": "./",
        "group": "vs"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "vsdocsdocs/**",
          "**/includes/**"
        ],
        "src": "./",
        "group": "vs"
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/visualstudio-docs",
      "feedback_product_url": "https://developercommunity2.visualstudio.com/search?space=8",
      "breadcrumb_path": "/visualstudio/_breadcrumb/toc.json",
      "extendBreadcrumb": "true",
      "manager": "jmartens",
      "audience": "developer",
      "defaultDevLang": "csharp",
      "ms.prod": "visual-studio-windows",
      "uhfHeaderId": "MSDocsHeader-VisualStudio",
      "titleSuffix": "Visual Studio (Windows)",
      "toc_preview": true,
      "searchScope": [
        "VS IDE"
      ],
      "featureFlags": [
        "enable_uhf_ppe"
      ],
      "archive_url": "/previous-versions/visualstudio",
      "recommendations": true
    },
    "fileMetadata": {
      "ms.prod": {
        "rtvs/**/**.md": "visual-studio-dev15"
      },
      "monikerRange": {
        "rtvs/**/**.md": "vs-2017"
      },
      "titleSuffix": {
        "ai/**/**.md": "AI tools for Visual Studio",
        "msbuild/**/**.md": "MSBuild"
      },
      "feedback_system": {
        "extensibility/**/*": "None"
      },
      "searchScope": {
        "extensibility/**/*.md": [
          "VS Extensibility"
        ],
        "msbuild/**/*.md": [
          "MSBuild"
        ]
      }
    },
    "template": [],
    "dest": "vsdocsdocs",
    "markdownEngineName": "markdig",
    "xref": [],
    "groups": {
      "vs": {
        "dest": "vs-dest",
        "moniker_range": ">= vs-2017"
      }
    }
  }
}
